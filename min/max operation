#design a data structure with min/max operation

from collections import deque
class myde:
    def __init__(self):
        self.dq=deque()
    def insertmax(self,x):
        self.dq.append(x)
    def insertmin(self,x):
        self.dq.appendleft(x)
    def popmax(self):
        return self.dq.pop()
    def popmin(self):
        return self.dq.popleft()
    def getmin(self):
        return self.dq[0]
    def getmax(self):
        return self.dq[-1]
c=myde()
c.insertmax(30)
c.insertmax(40)
c.insertmax(50)
c.insertmin(20)
print(c.popmax())
print(c.popmin())
print(c.getmin())
print(c.getmax())
